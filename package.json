{
  "name": "toi.vet",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.6",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "history": "5.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "react-sparklines-typescript": "1.3.0",
    "typescript": "4.4.4",
    "web-vitals": "2.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "react-scripts build && wrangler publish",
    "audit:fix": "npx yarn-audit-fix",
    "generate:api": "openapi-generator-cli generate -i https://toi-backend.herokuapp.com/swagger/v1/swagger.json --generator-name typescript-fetch --additional-properties=typescriptThreePlus=true -o src/api",
    "check:updates": "npx npm-check-updates"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead", 
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      ">0.3%",
      "not ie 11",
      "not dead", 
      "not op_mini all"
    ]
  },
  "devDependencies": {
    "@cloudflare/wrangler": "1.19.4",
    "@openapitools/openapi-generator-cli": "^2.4.13"
