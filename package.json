{
  "name": "toi.vet",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.14",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "history": "5.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "5.0.0",
    "react-sparklines-typescript": "1.3.0",
    "typescript": "4.5.5",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "react-scripts build && wrangler publish",
    "generate:api": "openapi-generator-cli generate -i https://backend.toi.vet/swagger/v1/swagger.json --generator-name typescript-fetch --additional-properties=typescriptThreePlus=true -o src/api",
    "generate:cert": "mkcert -key-file ./.cert/key.pem -cert-file ./.cert/cert.pem 'dev.toi.vet'",
    "audit:fix": "npx yarn-audit-fix",
    "check:updates": "npx npm-check-updates"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ]
  },
  "devDependencies": {
    "@cloudflare/wrangler": "1.19.7",
    "@openapitools/openapi-generator-cli": "^2.4.26"
  }
}
